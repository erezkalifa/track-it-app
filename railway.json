{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "trackIt-backend/Dockerfile"
  },
  "deploy": {
    "startCommand": "python trackIt-backend/run_server.py production",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
